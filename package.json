{
  "name": "autoui-react",
  "version": "0.0.2-alpha",
  "description": "Generate goal-oriented UIs in real-time using LLMs and your data schema",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "example": "cd example && npm run dev",
    "prepublishOnly": "npm run build",
    "setup-shadcn": "node ./scripts/setup-shadcn.js",
    "autoui:install-components": "npx shadcn@latest add --path=components/ui",
    "example-setup": "npm run build && cd examples/basic-demo && bash setup.sh"
  },
  "keywords": [
    "react",
    "ui",
    "ai",
    "llm",
    "generator"
  ],
  "author": "oblet",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oblet/autoui-react.git"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-radio-group": "^1.3.6",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.11",
    "ai": "^4.3.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.509.0",
    "openai": "^4.98.0",
    "react-hook-form": "^7.56.3",
    "tailwind-merge": "^3.2.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@tailwindcss/postcss": "^4.1.6",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^16.1.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.5.3",
    "prettier": "^2.8.8",
    "tailwindcss": "^4.1.6",
    "tsup": "^7.1.0",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.32.0",
    "vitest": "^0.32.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependenciesMeta": {
    "@shadcn/ui": {
      "optional": true
    },
    "tailwindcss": {
      "optional": true
    }
  }
}
